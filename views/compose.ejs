<%- include('partials/header'); -%>
<h1>Compose</h1>
<form method="post" action="/compose">

    <div class="form-group">
        <!-- Name input -->
    <div class="form-outline mb-4">
        <label class="form-label" for="form4Example1">Tiltle</label>
        <input type="text" id="form4Example1" class="form-control" name="posttitle" />
      </div>
    
    
      <!-- Message input -->
      <div class="form-outline mb-4">
        <label class="form-label" for="form4Example3">Input</label>
        <textarea class="form-control" id="form4Example3" rows="4" name="postinput"></textarea>
      </div>
    </div>
    
  
  
    <!-- Submit button -->
    <button type="submit" class="btn btn-primary">Publish</button>
  </form>

<%- include('partials/footer'); -%>